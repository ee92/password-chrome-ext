{
  "manifest_version": 2,

  "name": "HashPass",
  "description": "Generates your password using a master key.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
